entity Person {}

entity Nap {
    duration Integer required,
    rating Integer required min(0) max(10),
    day DayType
}

enum DayType {
	MONDAY, TUESDAY, WEDNESDAY, THURSDAY, FRIDAY
}

relationship OneToOne {
	Person{user} to User
}

relationship OneToMany {
	Person{naps} to Nap
}

paginate Sleep with infinite-scroll
